{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "This is monorepo boiler plate that using react and vite",
  "scripts": {
    "build": "pnpm recursive run build",
    "preview": "pnpm recursive run preview",
    "build:all": "pnpm clean && pnpm build",
    "build:main": "pnpm recursive --filter '@gan/main' run build",
    "build:shared": "pnpm recursive --filter '@gan/shared-*' run build",
    "clean": "rimraf ./packages/**/build && rimraf ./packages/**/lib && rimraf ./packages/**/tsconfig.tsbuildinfo",
    "dev": "pnpm recursive --filter '@gan/main' run dev",
    "lint": "pnpm recursive run lint",
    "lint:fix": "pnpm recursive run lint --ext js,jsx,mdx,ts,tsx --fix",
    "test": "pnpm recursive --filter '@gan/*' run test",
    "test:action": "pnpm recursive --filter '@gan/*' run test:action"
  },
  "author": "https://github.com/knguyen30111",
  "license": "MIT",
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.2",
    "@types/jest": "^27.4.0",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/testing-library__jest-dom": "^5.14.2",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitejs/plugin-legacy": "^1.6.4",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^27.4.6",
    "jest-environment-jsdom-fourteen": "^1.0.1",
    "jest-junit": "^13.0.0",
    "jest-watch-typeahead": "^1.0.0",
    "lodash-es": "^4.17.21",
    "prettier": "^2.5.1",
    "tslib": "2.2.0",
    "typescript": "4.2.4",
    "vite": "2.5.2",
    "vite-plugin-checker": "^0.3.4",
    "vite-plugin-compress": "^2.1.0",
    "vite-plugin-eslint": "^1.3.0",
    "vite-react-jsx": "^1.1.2",
    "vite-tsconfig-paths": "^3.3.17"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
